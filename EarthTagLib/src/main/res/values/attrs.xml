<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EarthTagView">
        <attr name="autoScrollMode">
            <enum name="disable" value="0"/>
            <enum name="decelerate" value="1"/>
            <enum name="uniform" value="2"/>
        </attr>
        <attr name="radiusPercent" format="float"/>
        <attr name="lightColor" format="color"/>
        <attr name="darkColor" format="color"/>
        <attr name="scale" format="float" />
        <attr name="minAlpha" format="float" />
        <attr name="maxAlpha" format="float" />
        <attr name="minVelocity" format="float" />
        <attr name="maxVelocity" format="float" />
    </declare-styleable>
</resources>